{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "remix build",
    "dev:remix": "remix watch",
    "dev:wrangler": "cross-env NODE_ENV=development npm run wrangler",
    "dev": "npm-run-all build --parallel \"dev:*\"",
    "start": "cross-env NODE_ENV=production npm run wrangler",
    "typecheck": "tsc",
    "db:seeds": "npx wrangler d1 execute DB --file ./db/schema.sql",
    "db:seeds:local": "npx wrangler d1 execute DB --file ./db/schema.sql --local",
    "wrangler": "wrangler pages dev ./public --ip 0.0.0.0 --local --persist"
  },
  "dependencies": {
    "@remix-run/cloudflare": "^1.15.0",
    "@remix-run/cloudflare-pages": "^1.15.0",
    "@remix-run/react": "^1.15.0",
    "@tsndr/cloudflare-worker-jwt": "^2.2.1",   
    "joi": "^17.8.3",
    "model-one": "^0.0.7",
    "stellar-base": "^8.2.2",
    "cross-env": "^7.0.3",
    "react": "^17",
    "react-dom": "^17"
  },
  "devDependencies": {
    "communi-design-system": "0.0.14",
    "@stellar/freighter-api": "latest",
    "@walletconnect/sign-client": "latest",
    "react-qrcode-logo": "latest",
    "@albedo-link/intent": "latest",
    "@cloudflare/workers-types": "^3.18.0",
    "@remix-run/dev": "^1.15.0",
    "@remix-run/eslint-config": "^1.15.0",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "eslint": "^8.27.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.8.4",
    "wrangler": "^2.2.1"
  },
  "engines": {
    "node": ">=16.15.1"
  },
  "volta": {
    "node": "16.15.1"
  }
}
