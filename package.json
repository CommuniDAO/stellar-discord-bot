{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "remix build",
    "dev:remix": "remix watch",
    "dev:wrangler": "cross-env NODE_ENV=development npm run wrangler",
    "dev": "npm-run-all build --parallel \"dev:*\"",
    "start": "cross-env NODE_ENV=production npm run wrangler",
    "typecheck": "tsc",
    "db:seeds": "npx wrangler d1 execute remixapptest --file ./db/schema.sql",
    "db:seeds:local": "npx wrangler d1 execute DB --file ./db/schema.sql --local",
    "wrangler": "wrangler pages dev ./public --ip 0.0.0.0 --local --persist"
  },
  "dependencies": {
    "@remix-run/cloudflare": "^1.14.3",
    "@remix-run/cloudflare-pages": "^1.14.3",
    "@remix-run/react": "^1.14.3",
    "@tsndr/cloudflare-worker-jwt": "^2.2.1",
    "buffer-polyfill": "npm:buffer@^6.0.3",
    "cfw-easy-utils": "^1.0.3",
    "cross-env": "^7.0.3",
    "joi": "^17.8.3",
    "model-one": "^0.0.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "stellar-base": "^8.2.2",
    "stellar-wallets-kit": "github:Creit-Tech/Stellar-Wallets-Kit"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.19.0",
    "@remix-run/dev": "^1.14.3",
    "@remix-run/eslint-config": "^1.14.3",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "better-sqlite3": "^8.1.0",
    "eslint": "^8.36.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.9.5",
    "wrangler": "^2.13.0"
  },
  "engines": {
    "node": ">=16.13.0"
  }
}
